#include<iostream>
using namespace std;

void sort012(int a[],int n)
{
    int mid,low=0;
    int high=n-1;
  while(mid<=high)
        {
            if(a[mid]==0)
            {
                swap(a[low],a[mid]);
                low++;
                mid++;
            }
            if(a[mid]==1)
            {
                mid++;
            }
            if(a[mid]==2)
            {
                swap(a[mid],a[high]);
                high--;
            }
        }
            


}

int main()
{

    int n;
    cin >>n;
    int a[n];

 for(int i =0;i<n;i++)
   {
       cin >>a[n];
   }

    sort012(a,n);

   for(int i =0;i<5;i++)
   {
       cout <<a[i]<<" ";
   }

}
