#include<iostream>
using namespace std;
void selectionsort(int a[],int n)
{
for(int i=0;i<n-1;i++)
{
 int min=i;
 for(int j=i+1;j<n;j++)
 {
 	if(a[j]<a[min])
 	{
 		min=j;
	}
 }
 if(min!=i)
 {
  int temp=a[min];
  a[min]=a[i];
  a[i]=temp;	
 }
}
}
int main()
{
    int n;
    cin>>n;
	int a[100];
	
	for(int i=0;i<n;i++)
	{
		cin>>a[i];
	}
	cout<<"unsorted array"<<endl;
	for(int i=0;i<n;i++)
	{
		cout<<a[i]<" ";
	}
cout<<endl;
	selectionsort(a,n);
    cout<<"sorted array"<<endl;
    for(int i=0;i<n;i++)
	{
		cout<<a[i]<<" ";
	}
	return 0;	
}
